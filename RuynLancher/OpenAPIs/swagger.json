{
  "openapi": "3.0.1",
  "info": {
    "title": "RuynServer",
    "version": "1.0"
  },
  "paths": {
    "/api/v1/LevelData": {
      "get": {
        "tags": [
          "LevelData"
        ],
        "operationId": "GetLevelList",
        "parameters": [
          {
            "name": "search",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "skip",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "take",
            "in": "query",
            "schema": {
              "maximum": 50,
              "minimum": 0,
              "type": "integer",
              "format": "int32",
              "default": 10
            }
          },
          {
            "name": "orderBy",
            "in": "query",
            "schema": {
              "$ref": "#/components/schemas/OrderByFilters"
            }
          },
          {
            "name": "decending",
            "in": "query",
            "schema": {
              "type": "boolean",
              "default": false
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/LevelListResponse"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/LevelListResponse"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/LevelListResponse"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "LevelData"
        ],
        "operationId": "AddLevelPack",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LevelData"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LevelData"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LevelData"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/v1/LevelData/{id}": {
      "get": {
        "tags": [
          "LevelData"
        ],
        "operationId": "GetLevelPackById",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/LevelData"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LevelData"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/LevelData"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "LevelData"
        ],
        "operationId": "DeleteLevelPack",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LevelData": {
        "required": [
          "author",
          "downloadCount",
          "fileData",
          "levelCount",
          "levelPackName"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "levelPackName": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "author": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "levelCount": {
            "type": "integer",
            "format": "int32"
          },
          "downloadCount": {
            "type": "integer",
            "format": "int32"
          },
          "fileData": {
            "type": "string",
            "format": "byte"
          },
          "uploadDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "LevelListResponse": {
        "required": [
          "author",
          "downloadCount",
          "levelCount",
          "levelPackName"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "levelPackName": {
            "type": "string",
            "nullable": true
          },
          "author": {
            "type": "string",
            "nullable": true
          },
          "levelCount": {
            "type": "integer",
            "format": "int32"
          },
          "downloadCount": {
            "type": "integer",
            "format": "int32"
          },
          "uploadDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "OrderByFilters": {
        "enum": [
          "UploadedDate",
          "DownloadCount",
          "LevelCount",
          "name",
          "author"
        ],
        "type": "string"
      }
    }
  }
}